{
  "name": "ibm-singapore-garage-dev-bootcamp",
  "version": "1.0.0",
  "description": "scaffold for the Bluemix Garage Developer Bootcamp",
  "main": "index.js",
  "scripts": {
    "start": "node server/server",
    "clear": "node ./clear.js",
    "dev": "gulp",
    "dist": "gulp dist",
    "lint": "esw -f simple-detail web server",
    "lint:ci": "npm run lint",
    "lint:watch": "nodemon 2>/dev/null  --exec 'npm run lint'",
    "server:coverage": "nyc --reporter=lcov --reporter=text --require babel-register npm run spec",
    "spec": "mocha --compilers js:babel-register --reporter spec --growl --recursive ./test-helper.js  **/*server.spec.js",
    "spec:ci": "npm run server:coverage",
    "spec:clear": "clear && npm run spec",
    "spec:watch": "nodemon 2>/dev/null  --exec 'npm run spec:clear || true'",
    "tdd": "npm-run-all --parallel *:watch",
    "test:watch": "karma start karma.conf.js",
    "test": "karma start karma.conf.coverage.js",
    "ci": "npm-run-all --parallel *:ci",
    "test:ci": "mkdir -p generated && node_modules/karma/bin/karma start karma.conf.saucelabs.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wpannell/ibm-singapore-garage-dev-bootcamp.git"
  },
  "keywords": [
    "node"
  ],
  "author": "IBM",
  "license": "IBM",
  "bugs": {
    "url": "https://github.com/wpannell/ibm-singapore-garage-dev-bootcamp/issues"
  },
  "homepage": "https://github.com/wpannell/ibm-singapore-garage-dev-bootcamp#readme",
  "engines": {
    "node": "7.6.0"
  },
  "dependencies": {
    "angular": "1.5.9",
    "angular-animate": "1.5.9",
    "angular-aria": "1.5.9",
    "angular-material": "1.1.3",
    "angular-ui-router": "0.4.1",
    "basic-auth": "1.0.3",
    "body-parser": "^1.15.2",
    "cfenv": "1.0.x",
    "express": "4.14.0",
    "fs-promise": "1.0.0",
    "immutable": "3.8.1",
    "jquery": "2.1.4",
    "lowdb": "^0.13.1",
    "moment": "2.17.1",
    "mongodb": "https://registry.npmjs.org/mongodb/-/mongodb-2.2.16.tgz",
